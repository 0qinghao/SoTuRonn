  booktitle  = {2020 {{IEEE International Conference}} on {{Image Processing}} ({{ICIP}})},
  author     = {Pakdaman, F. and Adelimanesh, M. A. and Gabbouj, M. and Hashemi, M. R.},
  year       = {2020},
