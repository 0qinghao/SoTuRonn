  file       = {E\:\\Documents\\Zotero\\storage\\PLI72HHI\\2018 - Enhanced Cross-Component Linear Model for Chroma Intra-Prediction in Video Coding - Zhang 等。.pdf;E\:\\Documents\\Zotero\\storage\\NALNIH5K\\8350031.html},
